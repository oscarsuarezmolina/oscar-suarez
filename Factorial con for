#include "stdafx.h"
#include "iostream"
#include "conio.h"

using namespace std;


void main()
{ int i,n,factorial, num;
   cout << " ingrese las veces a repetir: ";
   cin >> num;
   for (int j=1; j<=num ; i++)
   { cout << " digite el valos de n: ";
     cin >> n;
	 factorial = 1;
	 for (int i=1; i<=n ; i++)
	 { factorial = factorial * i;
	 }
	 cout << " el factorial es: " << factorial;
	 getch();
   }
	
}
